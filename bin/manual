#!/usr/bin/env node

/**
 *  Example of defining and raising errors.
 */
require('ttycolor')().defaults();
var clierr = require('..');
var Error = clierr.CliError;
var err = new Error('%s not found', 128, ['file.json']);
// print formatted message to stderr
err.error();
// use the error exit status code
err.exit();
